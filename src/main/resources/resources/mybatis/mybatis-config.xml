		<typeAlias type="me.qyh.mybatis.handler.FileStorageTypeHandler"
			alias="FileStorageTypeHandler" />
		<typeAlias type="me.qyh.mybatis.handler.TagSetTypeHandler"
			alias="TagSetTypeHandler" />

		<typeAlias type="me.qyh.entity.User" alias="User" />
		<typeAlias type="me.qyh.entity.Role" alias="Role" />
		<typeAlias type="me.qyh.entity.UserCode" alias="UserCode" />
		<typeAlias type="me.qyh.entity.RoleEnum" alias="RoleEnum" />
		<typeAlias type="me.qyh.entity.LoginInfo" alias="LoginInfo" />

		<typeAlias type="me.qyh.entity.MyFile" alias="MyFile" />
