		<typeAlias type="me.qyh.entity.message.MessageDetail" alias="MessageDetail" />
		<typeAlias type="me.qyh.entity.message.MessageStatus" alias="MessageStatus" />

		<typeAlias type="me.qyh.entity.Comment" alias="Comment" />
		<typeAlias type="me.qyh.entity.CommentScope" alias="CommentScope" />

		<typeAlias type="me.qyh.entity.Scope" alias="Scope" />

		<typeAlias type="me.qyh.entity.blog.BlogCategory" alias="BlogCategory" />
		<typeAlias type="me.qyh.entity.blog.Blog" alias="Blog" />
		<typeAlias type="me.qyh.entity.blog.BlogStatus" alias="BlogStatus" />
		<typeAlias type="me.qyh.entity.blog.BlogFrom" alias="BlogFrom" />
		<typeAlias type="me.qyh.entity.blog.TemporaryBlog" alias="TemporaryBlog" />
		<typeAlias type="me.qyh.entity.blog.BlogTag" alias="BlogTag" />

		<typeAlias type="me.qyh.entity.Space" alias="Space" />
