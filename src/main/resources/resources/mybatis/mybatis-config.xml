		<typeAlias type="me.qyh.page.LocationWidget" alias="LocationWidget" />
		<typeAlias type="me.qyh.page.widget.Widget" alias="Widget" />
		<typeAlias type="me.qyh.page.widget.config.WidgetConfig"
			alias="WidgetConfig" />
		<typeAlias type="me.qyh.page.widget.config.support.BlogWidgetConfig"
			alias="BlogWidgetConfig" />
		<typeAlias type="me.qyh.page.widget.config.support.BlogWidgetDisplayMode"
			alias="BlogWidgetDisplayMode" />


		<!--oauth2 -->
		<typeAlias type="me.qyh.oauth2.entity.OauthUser" alias="OauthUser" />
		<typeAlias type="me.qyh.oauth2.entity.OauthType" alias="OauthType" />
	</typeAliases>
	<mappers>
		<mapper resource="resources/mybatis/mapper/roleMapper.xml" />
		<mapper resource="resources/mybatis/mapper/userMapper.xml" />
		<mapper resource="resources/mybatis/mapper/userCodeMapper.xml" />
		<mapper resource="resources/mybatis/mapper/loginInfoMapper.xml" />

		<mapper resource="resources/mybatis/mapper/fileMapper.xml" />

