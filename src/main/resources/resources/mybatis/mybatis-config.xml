		<mapper resource="resources/mybatis/mapper/blogWidgetConfigMapper.xml" />
		<mapper resource="resources/mybatis/mapper/locationWidgetMapper.xml" />

		<!--oauth2 -->
		<mapper resource="resources/mybatis/mapper-oauth2/oauthUserMapper.xml" />
